datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Feedback {
  id         String  @id @default(uuid()) //@id indica que Ã© a primary key
  type       String
  comment    String
  screenshot String?

  @@map("feedbacks")
}
